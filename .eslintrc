{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi":"off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "object-curly-spacing": "off",
        "indent": ["error", 4],
        "linebreak-style": "off",
        "operator-linebreak": "off",
        "max-len": ["error", { "code": 120 }]
    },

    "extends": ["eslint:recommended", "google"]
}